<Window x:Class="HDTelefoonKosten.Windows.MargeManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HDTelefoonKosten.Windows"
        mc:Ignorable="d"
        Title="MargeManagerWindow" Height="238" Width="350">
    <Grid>
        <ListBox x:Name="lbAllCompanies" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,36,0,0" HorizontalAlignment="Left" Width="196" Height="175" VerticalAlignment="Top"/>
        <Button x:Name="btnSetMargeForCompany" Content="Zet Marge" HorizontalAlignment="Left" Margin="229,77,0,0" VerticalAlignment="Top" Height="31" Width="109" Click="btnSetMargeForCompany_Click"/>
        <TextBox x:Name="tbPermanentMarge" HorizontalAlignment="Left" Margin="229,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="58" PreviewTextInput="tbPermanentMarge_PreviewTextInput"/>
        <Label Content="%" HorizontalAlignment="Left" Margin="285,46,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.053,-0.14"/>

    </Grid>
</Window>
